{
  "name": "bit-server",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Daniel <triotk@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@typegoose/typegoose": "^7.4.8",
    "apollo-server-express": "^2.21.0",
    "mongoose": "5.10.18",
    "type-graphql": "^1.1.1",
    "typeorm": "^0.2.31"
  },
  "devDependencies": {
    "@types/mongoose": "^5.10.3",
    "typescript": "^4.2.2"
  },
  "bit": {
    "env": {
      "compiler": {
        "bit.envs/compilers/typescript@3.1.53": {
          "rawConfig": {
            "tsconfig": {
              "compilerOptions": {
                "target": "ES5",
                "module": "CommonJS",
                "skipLibCheck": true,
                "emitDecoratorMetadata": true,
                "experimentalDecorators": true,
                "typeRoots": [
                  "./src/typed"
                ]
              }
            }
          }
        }
      }
    },
    "componentsDefaultDirectory": "components/{name}",
    "packageManager": "yarn"
  }
}
